 </function>
 
 <function>
 	<name>populateTemplateWithBag</name>
 	<function-class>org.intermine.api.template.TemplatePopulator</function-class>
 	<function-signature>
 		org.intermine.template.TemplateQuery populateTemplateWithBag(org.intermine.template.TemplateQuery, org.intermine.api.profile.InterMineBag )
 	</function-signature>
  </function>	

 <function>
 	<name>populateTemplateWithObject</name>
 	<function-class>org.intermine.api.template.TemplatePopulator</function-class>
 	<function-signature>
 		org.intermine.template.TemplateQuery populateTemplateWithObject(org.intermine.template.TemplateQuery, org.intermine.model.InterMineObject )
 	</function-signature>
  </function>
  
  
  <function>
